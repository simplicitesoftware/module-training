{
  "name": "training",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve --mode remote",
    "serve-dev": "vue-cli-service serve --mode local",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "zip": "jar -cfM ../../resources/ObjectExternal/TrnFront/site.zip -C ./dist/ .",
    "deploy": "npm run build && npm run zip"
  },
  "dependencies": {
    "@appbaseio/reactivesearch-vue": "^1.36.7",
    "click-outside-vue3": "^4.0.1",
    "core-js": "^3.36.0",
    "highlight.js": "^11.9.0",
    "pinia": "^2.1.7",
    "simplicite": "^3.0.1",
    "vue": "^3.2.47",
    "vue-meta": "^3.0.0-alpha.2",
    "vue-router": "^4.3.0"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.23.10",
    "@vue/cli-plugin-babel": "^5.0.8",
    "@vue/cli-plugin-eslint": "^5.0.8",
    "@vue/cli-service": "^5.0.8",
    "@vue/compiler-sfc": "^3.1.0",
    "eslint": "^8.57.0",
    "eslint-plugin-vue": "^9.22.0",
    "sass": "^1.71.1",
    "sass-loader": "^8.0.0",
    "webpack": "^5.90.3"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/vue3-essential",
      "eslint:recommended"
    ],
    "rules": {
      "vue/multi-word-component-names": 0
    },
    "parserOptions": {
      "parser": "@babel/eslint-parser"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ]
}
